<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

    <bean id="addressBook" class="pojos.AddressBook">
        <property name="fullName" value="123_TestSubway"/>
        <property name="address" value="1234 Main Street"/>
        <property name="city" value="florida"/>
        <property name="zipCode" value="65456"/>
        <property name="phoneNum" value="4441231234"/>
        <property name="state" value=" Alabama "/>
    </bean>

</beans>

